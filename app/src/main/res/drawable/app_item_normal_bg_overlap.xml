<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="6dp"
        android:top="6dp">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />   <!-- 设置圆角弧度 -->
            <solid android:color="#1d1e1f" />   <!-- 设置背景颜色 -->
        </shape>
    </item>
    <item
        android:bottom="6dp"
        android:right="6dp">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />   <!-- 设置圆角弧度 -->
            <solid android:color="#252627" />   <!-- 设置背景颜色 -->
        </shape>
    </item>


</layer-list>